{
  "output": {
    "filePath": "context-full.xml",
    "style": "xml",
    "showLineNumbers": true
  },
  "ignore": {
    "customPatterns": [
      "node_modules/**",
      ".git/**",
      ".cursor/**",
      "**/target/**",
      "**/node_modules/**",
      "**/dist/**",
      "**/.npm-cache/**",
      "**/.env",
      "**/.env.*",
      "context*.xml",
      "context-*.xml",
      "*.log",
      ".DS_Store",
      "**/*.bin",
      "**/*.json.bak",
      "**/*.json.tmp"
    ]
  },
  "include": [
    "**/*"
  ]
}
